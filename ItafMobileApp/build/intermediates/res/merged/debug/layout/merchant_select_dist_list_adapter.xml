<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/rela_adapter_layout" >

    <itaf.mobile.app.ui.widget.AsyncImageView
        android:id="@+id/aiv_msdla_merchant_ico"
        style="@style/aiv_96" />

    <LinearLayout
        style="@style/linear_common"
        android:layout_toRightOf="@+id/aiv_msdla_merchant_ico" >

        <LinearLayout style="@style/linear_padding1" >

            <TextView
                android:id="@+id/tv_msdla_company_name"
                style="@style/tv_link" />
        </LinearLayout>

        <LinearLayout style="@style/linear_padding1" >

            <TextView
                style="@style/tv_item_left"
                android:text="距离" />

            <TextView
                android:id="@+id/tv_msdla_service_coverage"
                style="@style/tv_item_right" />
        </LinearLayout>

        <LinearLayout style="@style/linear_padding1" >

            <TextView
                style="@style/tv_item_left"
                android:text="地址" />

            <TextView
                android:id="@+id/tv_msdla_company_address"
                style="@style/tv_item_right" />
        </LinearLayout>

        <LinearLayout style="@style/linear_padding1" >

            <Button
                android:id="@+id/tv_msdla_select"
                style="@style/app_top_btn_right"
                android:text="@string/str_common_select_dist" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>